{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "env": {
      "DATABASE_URL": "@DATABASE_URL",
      "PORT": "3000",
      "JWT_SECRET": "@JWT_SECRET"
    }
  }
  